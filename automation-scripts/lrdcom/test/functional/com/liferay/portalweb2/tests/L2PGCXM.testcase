<definition component-name="support-qa">
	<property name="testray.main.component.name" value="Support QA" />

	<var name="code" value="acmeportal" />
	<var name="lesaURL" value="/group/customer" />

	<set-up>
		<execute macro="User#login">
			<var method="TestPropsUtil#get('lesa2.email.address.10')" name="userEmailAddress" />
			<var method="TestPropsUtil#get('lesa2.email.password.10')" name="userPassword" />
		</execute>

		<execute function="Open" locator1="${lesaURL}" />

		<execute macro="GoToLESA#goToLESA" />

	</set-up>
	
	<command name="AddCallLog" priority="4">
		<var method="TestPropsUtil#get('account.5')" name="account" />
		
		<execute function="Click" locator1="L2Home#SEARCH" />
		<execute function="KeyPress" locator1="L2Search#SEARCH_FIELD" value1="acme" />
		<execute function="KeyPress" locator1="L2Search#SEARCH_FIELD" value1="\RETURN" />
		<execute function="Click" locator1="L2Search#PROJECTS_FILTER" />
		<execute function="Click#clickAtAndWait" locator1="L2Search#PROJECT_NAME" />
		<execute function="Click" locator1="L2AccountDetails#CALL_LOG_TAB" />
		<execute function="Click" locator1="L2AccountDetails#ADD_CALL_LOG_BUTTON" /> 
		<execute function="SelectFrame" locator1="L2AccountDetails#CALL_LOG_IFRAME" />
		
		<execute function="Select" locator1="L2AccountDetails#CALL_TYPE_DROPDOWN" value1="Incoming" />
		<execute function="Select" locator1="L2AccountDetails#CALL_DATE_MONTH" value1="January" />
		<execute function="Select" locator1="L2AccountDetails#CALL_DATE_DAY" value1="6" />
		<execute function="Select" locator1="L2AccountDetails#CALL_DATE_YEAR" value1="2016" />
		<execute function="Select" locator1="L2AccountDetails#CALL_LENGTH_HOURS" value1="0" />
		<execute function="Select" locator1="L2AccountDetails#CALL_LENGTH_MINUTES" value1="45" />
		<execute function="Select" locator1="L2AccountDetails#CALL_LENGTH_SECONDS" value1="10" />
		<execute function="Type" locator1="L2AccountDetails#SUMMARY_FIELD" value1="Resolving clustering issues" />
		<execute function="Type" locator1="L2AccountDetails#CLIENTS_PRESENT_FIELD" value1="Hannibal Smith, B.A. Baracus, Templeton Peck, H. Murdoch" />
		<execute function="Type" locator1="L2AccountDetails#NOTES_FIELD" value1="Hire the A-Team" />
		<execute function="Type" locator1="L2AccountDetails#ACTION_ITEMS_FIELD" value1="Escape to LA" />
		<execute function="Click" locator1="L2AccountDetails#SAVE_BUTTON" />
		<execute function="SelectFrame" value1="relative=top" />
	</command>
	
	<command name="AddCASInfo" priority="4">
		var method="TestPropsUtil#get('account.5')" name="account" />
		
		<execute function="Click" locator1="L2Home#SEARCH" />
		<execute function="KeyPress" locator1="L2Search#SEARCH_FIELD" value1="acme" />
		<execute function="KeyPress" locator1="L2Search#SEARCH_FIELD" value1="\RETURN" />
		<execute function="Click" locator1="L2Search#PROJECTS_FILTER" />
		<execute function="Click#clickAtAndWait" locator1="L2Search#PROJECT_NAME" />
		<execute function="Click" locator1="L2AccountDetails#CAS_INFORMATION_TAB" />
		<execute function="ScrollWebElementIntoView" locator1="L2AccountDetailsCAS#EDIT_BUTTON" />
		<execute function="Click" locator1="L2AccountDetailsCAS#EDIT_BUTTON" />
		<execute function="Type" locator1="L2AccountDetailsCAS#REASONS_FOR_STRATEGIC_FIELD" value1="big manufacturing company, introduced to us by RH" />
		<execute function="KeyPress" locator1="L2AccountDetailsCAS#PROJECT_OVERVIEW_FIELD" value1="Liferay Version: 6.2.10 EE" />
		<execute function="KeyPress" locator1="L2AccountDetailsCAS#PROJECT_OVERVIEW_FIELD" value1="\RETURN" />
		<execute function="KeyPress" locator1="L2AccountDetailsCAS#PROJECT_OVERVIEW_FIELD" value1="RHEL JBoss EAP 6.4" />
		<execute function="Type" locator1="L2AccountDetailsCAS#CONTACT_OVERVIEW_FIELD" value1="Contact FN-2187" />
		<execute function="Type" locator1="L2AccountDetailsCAS#MILESTONES_FIELD" value1="Building a Death Star by 2020" />
		<execute function="Click" locator1="L2AccountDetailsCAS#SAVE_BUTTON" />
	</command>
	
	<command name="AddCEMCXMInfo" priority="4">
		<var method="TestPropsUtil#get('account.5')" name="account" />
		
		<execute function="Click" locator1="L2Home#SEARCH" />
		<execute function="KeyPress" locator1="L2Search#SEARCH_FIELD" value1="acme" />
		<execute function="KeyPress" locator1="L2Search#SEARCH_FIELD" value1="\RETURN" />
		<execute function="Click" locator1="L2Search#PROJECTS_FILTER" />
		<execute function="Click#clickAtAndWait" locator1="L2Search#PROJECT_NAME" />
		<execute function="Click" locator1="L2AccountDetails#CEM_CXM_INFORMATION_TAB" />
		<execute function="ScrollWebElementIntoView" locator1="L2AccountDetailsCEMCXM#EDIT_BUTTON" />
		<execute function="Click" locator1="L2AccountDetailsCEMCXM#EDIT_BUTTON" />
		<execute function="KeyPress" locator1="L2AccountDetailsCEMCXM#LICENSE_INFORMATION_FIELD" value1="Platinum Prod (5); Non-Prod (2)" />
		<execute function="KeyPress" locator1="L2AccountDetailsCEMCXM#LICENSE_INFORMATION_FIELD" value1="\RETURN" />
		<execute function="KeyPress" locator1="L2AccountDetailsCEMCXM#LICENSE_INFORMATION_FIELD" value1="\RETURN" />
		<execute function="KeyPress" locator1="L2AccountDetailsCEMCXM#LICENSE_INFORMATION_FIELD" value1="Support Expires January 4, 2017" />
		
		<execute function="Type" locator1="L2AccountDetailsCEMCXM#SALESFORCE_URL_FIELD" value1="Insert Salesforce link here." />
		<execute function="Type" locator1="L2AccountDetailsCEMCXM#TRAININGS_ATTENDED_FIELD" value1="Liferay Certification" />
		<execute function="Click" locator1="L2AccountDetailsCEMCXM#SAVE_BUTTON" />
	</command>
</definition>