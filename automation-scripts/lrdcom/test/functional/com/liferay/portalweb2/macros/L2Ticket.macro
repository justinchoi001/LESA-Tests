	<command name="CreateTicketWithExistingDetails">
		<execute function="Click" locator1="L2Home#NEW_TICKET" />

		<execute function="Select" locator1="L2AddEnvDetails#SELECT_PROJECT" value1="${account}" />
		<execute function="Select" locator1="L2AddEnvDetails#SELECT_PRODUCT" value1="Portal Production" />
		<execute function="Select" locator1="L2AddEnvDetails#SELECT_SAVED_ENV_CONFIG" value1="${envDetailsName}" />
		<execute function="Select#selectAndWait" locator1="L2AddEnvDetails#SELECT_COMPONENT" value1="${component}" />
		<execute function="Select" locator1="L2AddEnvDetails#BROWSER" value1="${browser}" />
		<execute function="Check" locator1="L2AddEnvDetails#PORTAL_EXT_CHECKBOX" />
		<execute function="Check" locator1="L2AddEnvDetails#PATCH_LEVEL_CHECKBOX" />
		<execute function="Select" locator1="L2AddEnvDetails#SYSTEM_STATUS_DROPDOWN" value1="${systemStatus}" />
		<execute function="Type" locator1="L2AddEnvDetails#SUMMARY_FIELD" value1="${summary}" />
		<execute function="Type" locator1="L2AddEnvDetails#DESCRIPTION_FIELD" value1="${description}" />

		<execute function="Click" locator1="L2AddEnvDetails#CREATE_BUTTON" value1="Create" />
		<execute function="AssertTextEquals" locator1="L2Ticket#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

