	<command name="ForwardTicket">
		<var name="key_summary" value="${summary}" />

		<execute function="DoubleClick" locator1="L2Home#FORWARDING_TICKET" />
		<execute function="Pause" locator1="2000" />
		<execute function="Click" locator1="L2Ticket#THREE_DOT_DROPDOWN" />
		<execute function="Click" locator1="L2Ticket#FORWARD_TICKET" />
		<execute function="Click" locator1="L2Ticket#YES_BUTTON" />
		<execute function="Type" locator1="L2Ticket#MAIN_GOAL_FIELD" value1="This is the main goal: Drink water" />
		<execute function="Type" locator1="L2Ticket#WORK_DONE_FIELD" value1="This is the work done field: buy cups" />
		<execute function="Type" locator1="L2Ticket#CUSTOMER_NEXT_STEPS_FIELD" value1="Customer's next steps: buy straws" />
		<execute function="Type" locator1="L2Ticket#NEXT_REGION_NEXT_STEPS_FIELD" value1="This is the next region field: buy water" />
		<execute function="Click" locator1="L2Ticket#IFRAME_FORWARD_BUTTON" />
		<execute function="AssertTextEquals" locator1="L2Ticket#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>
