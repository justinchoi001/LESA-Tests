<definition>
	<command name="AcceptSolution">
		<execute function="Click#clickAt" locator1="L2Home#PRIMARY_ASSIGNEE_SOLUTION_PROPOSED_TICKET" />

		<if>
			<condition function="IsElementPresent" locator1="L2Ticket#SOLUTION_PROPOSED_TAB" />
			<then>
				<execute function="Click" locator1="L2Ticket#SOLUTION_PROPOSED_TAB" />
			</then>
			<else>
				<execute function="AssertElementPresent" locator1="L2Ticket#SOLUTION_YES_BUTTON" />
				<execute function="AssertElementPresent" locator1="L2Ticket#SOLUTION_NO_BUTTON" />
				<execute function="AssertElementPresent" locator1="L2Ticket#SOLUTION_NOT_APPLICABLE_BUTTON" />
			</else>
		</if>

		<execute function="Click" locator1="L2Ticket#SOLUTION_YES_BUTTON" />

		<if>
			<condition function="IsElementPresent" locator1="L2Ticket#SOLUTION_PROPOSED_TAB" />
			<then>
				<execute function="Click" locator1="L2Ticket#SOLUTION_PROPOSED_TAB" />
			</then>
			<else>
				<execute function="AssertElementPresent" locator1="L2Ticket#SOLUTION_YES_BUTTON" />
				<execute function="AssertElementPresent" locator1="L2Ticket#SOLUTION_NO_BUTTON" />
				<execute function="AssertElementPresent" locator1="L2Ticket#SOLUTION_NOT_APPLICABLE_BUTTON" />
			</else>
		</if>

		<execute function="Click" locator1="L2Ticket#SOLUTION_YES_BUTTON" />
		<execute function="AssertTextEquals" locator1="L2Ticket#TICKET_STATUS" value1="Closed" />
		<execute function="AssertTextEquals" locator1="L2Ticket#TICKET_RESOLUTION" value1="(Completed)" />
	</command>

	<command name="ProposeSolution">
		<var name="summary" value="Solution Repo Test" />
		<var name="ticketStatus" value="Solution Proposed" />

		<execute function="Click#clickAtAndWait" locator1="L2Home#PRIMARY_ASSIGNEE_OPEN_TICKET_3" value1="${summary}" />

		<execute function="Click" locator1="L2Ticket#THREE_DOT_DROPDOWN" />
		<execute function="Click" locator1="L2Ticket#PROPOSE_SOLUTION" />

		<execute function="SelectWindow" locator1="title=Support" />
		<execute function="Type" locator1="SolutionsRepo#SOLUTION_SUMMARY" value1="This is a solution." />
		<execute function="ScrollWebElementIntoView" locator1="SolutionsRepo#ISSUE_SUMMARY_TOGGLE" value1="Customer's description does NOT accurately capture the issue" />
		<execute function="Select" locator1="SolutionsRepo#ISSUE_SUMMARY_TOGGLE" value1="Customer's description does NOT accurately capture the issue." />
		<execute function="Type" locator1="SolutionsRepo#ISSUE_SUMMARY" value1="This is a new summary and a new description." />
		<execute function="Click" locator1="SolutionsRepo#UPLOAD_BUTTON" />
		<execute function="Pause" locator1="5000" />

		<!--Users will have to click Cancel then Browse again before the test will proceed. -->

		<execute function="SikuliClick" locator1="OS#FILE_DIALOG_BOX_TYPE_FILE_NAME_ICON_PNG" />
		<execute function="SikuliType" locator1="OS#FILE_DIALOG_BOX_FILE_NAME_FIELD_PNG" value1="D:\Support Ops Repository\LESA\dependencies\solution.xml" />
		<execute function="SikuliClick" locator1="OS#FILE_DIALOG_BOX_OPEN_BUTTON_PNG" />
		<execute function="SelectWindow" locator1="title=Support" />
		<execute function="ClickNoError" locator1="SolutionsRepo#SEND_BUTTON" />
		<execute function="SelectWindow" locator1="null" />

		<execute function="AssertTextEquals" locator1="L2Ticket#TICKET_STATUS" value1="Solution Proposed" />
	</command>

	<command name="RejectSolution">
		<execute function="DoubleClick" locator1="L2Home#PRIMARY_ASSIGNEE_SOLUTION_PROPOSED_TICKET" />
		<execute function="Pause" locator1="25000" />

		<execute function="Click" locator1="L2Ticket#SOLUTION_NO_BUTTON" />
		<execute function="Select" locator1="L2Ticket#REJECT_REASON" value1="The fix did not deploy." />
		<execute function="Type" locator1="L2Ticket#REJECT_EXPLANATION" value1="Plugin not registering" />
		<execute function="Click" locator1="L2Ticket#REJECT_SEND_BUTTON" />
		<execute function="AssertTextEquals" locator1="L2Ticket#TICKET_STATUS" value1="Investigating" />
	</command>
</definition>